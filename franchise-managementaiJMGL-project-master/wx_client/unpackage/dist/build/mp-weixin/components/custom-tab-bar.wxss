.custom-tab-bar{position:fixed;bottom:0;left:0;right:0;height:200rpx;background-color:#fff;display:flex;box-shadow:0 -2rpx 10rpx rgba(0,0,0,.05);z-index:999}.custom-tab-bar .tab-item{flex:1;display:flex;flex-direction:column;align-items:center;justify-content:center}.custom-tab-bar .tab-item .tab-image{width:48rpx;height:48rpx;margin-bottom:6rpx}.custom-tab-bar .tab-item .tab-text{font-size:24rpx;color:#7a7e83}.custom-tab-bar .tab-item.active .tab-text{color:#0a2576}